@model BugTrackerWithLayout.ViewModels.UserProfileViewModel

@{
    ViewBag.Title = "Hesabım";
}

<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">Kullanıcı Bilgileri</h3>
    </div>
    <div class="panel-body">
        <p><strong>Kullanıcı Adı:</strong> @Model.Username</p>
        <p><strong>E-posta:</strong> @Model.Email</p>
        <p><strong>Rol:</strong> @Model.Role</p>
    </div>
</div>
